package Matemstika;

/**
 *
 * @author tamara
 */
public class Matematika {
    // method pertambahan
    public int pertambahan(int a, int b){
        return a + b;
    }
    // method perkalian
    public int perkalian(int a, int b){
        return a * b;
    }
}